<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wust.ws.animonpredraw" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ValueAnimationActivity"
            android:label="@string/title_activity_value_animation" >
        </activity>
        <activity
            android:name=".AnimationSetActivity"
            android:label="@string/title_activity_animation_set" >
        </activity>
        <activity
            android:name=".XmlAnimatonActivity"
            android:label="@string/title_activity_xml_animaton" >
        </activity>
        <activity
            android:name=".LayoutAnimationActivity"
            android:label="@string/title_activity_layout_animation" >
        </activity>
        <activity
            android:name=".ViewAnimatonActivity"
            android:label="@string/title_activity_view_animaton" >
        </activity>
    </application>

</manifest>
